<template id="import-form-template">
    <div class="import-form">
        <form class="form-horizontal">
            <div class="form-group">
                <div class="col-md-offset-4 col-md-8">
                    <button type="submit" data-bind="click: importUsers" class="btn btn-primary">Import</button>
                </div>
            </div>

            <div data-bind="foreach: users">
                <user-editable params="{ user: $data, id: $index }"></user-editable>
            </div>

            <div class="form-group">
                <div class="col-md-offset-4 col-md-8">
                    <button type="submit" data-bind="click: importUsers" class="btn btn-primary">Import</button>
                </div>
            </div>
        </form>
    </div>
</template>
