<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="icon" href="data:image/png;base64," type="image/png" src="data:image/png;base64,">
    <link type="text/css" rel="stylesheet" href="css/bootstrap.min.css">
    <link type="text/css" rel="stylesheet" href="css/bootstrap-material-design.min.css">
    <link type="text/css" rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/icon?family=Material+Icons">
    <title>Teamwork Chat Slack importer</title>
</head>
<body>
    <div class="main-header container-fluid">
        <div class="row">
            <div class="col-xs-6">
                <img src="//cdn0.teamwork.chat/0.19.1/images/logo-horizontal-beta.svg">
                <h1>Slack importer</h1>
            </div>
        </div>
    </div>
    <div class="container">
        <!-- 1st step: Login to teamwork -->
        <div class="main-content" data-bind="ifnot: teamworkLoggedIn()">
            <teamwork-login-form params="{
                apiState: apiState,
                teamworkLoggedIn: teamworkLoggedIn,
                siteUrl: siteUrl
            }"></teamwork-login-form>
        </div>
        <!-- 2nd step: Import -->
        <div class="main-content" data-bind="if: teamworkLoggedIn() && !importDone()">
            <import-form params="{ apiState: apiState, importDone: importDone }"></import-form>
        </div>
        <!-- 3rd step: Button to redirect to Teamwork Chat -->
        <div class="main-content" data-bind="if: importDone()">
            <import-done></import-done>
        </div>
    </div>

    <loading-overlay params="apiState: apiState"></loading-overlay>

    <!-- Teamwork login form component template -->
    <% include teamwork_login_form %>
    <!-- Import form component template -->
    <% include import_form %>
    <!-- User editable component template -->
    <% include user_editable %>
    <!-- Loading overlay component template -->
    <% include loading_overlay %>
    <!-- Import done component template -->
    <% include import_done %>

    <script src="ext_lib/jquery.min.js"></script>
    <script src="ext_lib/material.min.js"></script>
    <script src="ext_lib/ripples.min.js"></script>
    <script src="ext_lib/bootstrap.min.js"></script>
    <script src="bundle.js"></script>
</body>
</html>
